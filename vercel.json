{
    "rewrites": [
      {
        "source": "/auth/(.*)",
        "destination": "/auth/$1"
      },
      {
        "source": "/api/auth/(.*)",
        "destination": "/api/auth/$1"
      },
      {
        "source": "/profile",
        "destination": "/profile"
      },
      {
        "source": "/cree",
        "destination": "/cree"
      }
    ]
  }
  